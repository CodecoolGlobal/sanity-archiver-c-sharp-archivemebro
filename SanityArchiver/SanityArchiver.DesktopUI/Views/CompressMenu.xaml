<Window x:Class="SanityArchiver.DesktopUI.Views.CompressMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SanityArchiver.DesktopUI.Views"
        mc:Ignorable="d"
        Title="CompressMenu" Height="120" Width="300">
    <Grid x:Name="CompressGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label Grid.Column="0" Grid.Row="0" Margin="2,10,8.2,4.6">Compressed File Name:</Label>
        <TextBox x:Name="CompressedFileName" Grid.Row="0" Margin="7.8,8,7.6,8.6" Grid.Column="1"/>
        <Button Grid.Column="0" Grid.Row="1" Margin="8" Click="Save_Click">Save</Button>
        <Button Grid.Column="1" Grid.Row="1" Margin="8" Click="Cancel_Click">Cancel</Button>
    </Grid>
</Window>
