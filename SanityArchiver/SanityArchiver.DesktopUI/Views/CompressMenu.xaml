<Window x:Class="SanityArchiver.DesktopUI.Views.CompressMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SanityArchiver.DesktopUI.Views"
        mc:Ignorable="d"
        Title="CompressMenu" Height="450" Width="800">
    <Grid x:Name="CompressGrid">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
           <RowDefinition/>
       </Grid.RowDefinitions>
        <Label Grid.Column="0" Grid.Row="0">Compressed File Name:</Label>
        <TextBox Grid.Column="1" Grid.Row="0" Text="{Binding Path=CompressedFile.Name}"/>
        <Button Grid.Column="0" Grid.Row="1" Click="Button_Click">Save</Button>
        <Button Grid.Column="1" Grid.Row="1" Click="Button_Click_1">Cancel</Button>
    </Grid>
</Window>
