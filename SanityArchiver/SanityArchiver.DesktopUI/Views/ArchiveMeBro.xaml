<Window x:Class="SanityArchiver.DesktopUI.Views.ArchiveMeBro"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SanityArchiver.DesktopUI"
        mc:Ignorable="d"
        Title="Archive My Files" Height="597" Width="600" ResizeMode="NoResize">
    <Grid>
        <TreeView x:Name="DirectoryMap" HorizontalAlignment="Left" Height="473" Margin="10,56,0,0" VerticalAlignment="Top" Width="311" TreeViewItem.Collapsed="ItemCollapseHandler" TreeViewItem.Expanded="ItemExpandedHandler" TreeViewItem.Selected="ItemSelected"/>
        <Button x:Name="Encrypt" Content="Encrypt Files" HorizontalAlignment="Left" Margin="415,56,0,0" VerticalAlignment="Top" Width="93" Click="Encrypt_Click" />
        <Button x:Name="Archive" Content="Archive Files" HorizontalAlignment="Left" Margin="415,138,0,0" VerticalAlignment="Top" Width="93" Click="Archive_Click"/>
        <Button x:Name="Decrypt" Content="Decrypt Files" HorizontalAlignment="Left" Margin="415,98,0,0" VerticalAlignment="Top" Width="93" Click="Decrypt_Click" />
        <Button x:Name="Archive_Copy" Content="Archive Copy" HorizontalAlignment="Left" Margin="415,176,0,0" VerticalAlignment="Top" Width="93" Click="ClickMe"/>
    </Grid>
</Window>
